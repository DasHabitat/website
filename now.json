{
  "version": 2,
  "name": "website",
  "scope": "habitat",
  "alias": "das-habitat.de",
  "public": false,
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build",
      "config": { "distDir": "dist" }
    }
  ],
  "routes": [
    { "src": "/reparieren", "status": 301, "headers": { "Location": "https://www.notion.so/dashabitat/Repair-Caf-im-Habitat-a97680b273284d63ab28d65a773fb326" } },
    { "src": "/reparieren/", "status": 301, "headers": { "Location": "https://www.notion.so/dashabitat/Repair-Caf-im-Habitat-a97680b273284d63ab28d65a773fb326" } },
    { "src": "/imprint.html", "status": 301, "headers": { "Location": "/impressum/" } },
    { "src": "^/assets/(.*)", "headers": {"cache-control": "max-age=31536000,immutable"} },
    { "src": "^/images/(.*)", "headers": {"cache-control": "max-age=31536000,immutable"} },
    { "src": "^/(.*).html", "headers": {"cache-control": "public,max-age=0,must-revalidate"} },
    { "handle": "filesystem" },
    { "src": "/.*", "status": 404, "dest": "404.html" }
  ],
  "github": {
    "silent": true
  }
}
