{
  "name": "website",
  "version": "2.0.0",
  "description": "Kirby powered website for Das Habitat Augsburg e.V.",
  "main": "index.js",
  "repository": "https://github.com/das-habitat/website",
  "author": "Florian Pichler for Das Habitat Augsburg e.V.",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "gulp",
    "composer": "cd kirby && composer",
    "lint": "npm-run-all -p -c \"lint:!(fix)\"",
    "lint:php": "prettier \"**/*.php\" --check",
    "lint:css": "prettier \"**/*.php\" --check",
    "lint:js": "prettier \"**/*.php\" --check",
    "lint:fix": "npm-run-all -p -c \"lint:*:fix\"",
    "lint:php:fix": "prettier \"**/*.php\" --write",
    "lint:css:fix": "prettier \"**/*.php\" --write",
    "lint:js:fix": "prettier \"**/*.php\" --write",
    "gulp": "gulp",
    "kirby": "cd kirby && php -S 127.0.0.1:8000 kirby/router.php",
    "kirby:win": "cd .\\kirby && php -S 127.0.0.1:8000 .\\kirby\\router.php",
    "build": "gulp build"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@babel/register": "^7.13.8",
    "@prettier/plugin-php": "^0.16.1",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "autoprefixer": "^10.2.5",
    "browser-sync": "^2.26.14",
    "cross-env": "^7.0.3",
    "cssnano": "^4.1.10",
    "fibers": "^5.0.0",
    "gulp": "^4.0.2",
    "gulp-cli": "^2.3.0",
    "gulp-postcss": "^9.0.0",
    "gulp-sass": "^4.1.0",
    "gulp-sourcemaps": "^3.0.0",
    "normalize.css": "^8.0.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.8",
    "postcss-inline-svg": "^5.0.0",
    "prerender_all": "^1.1.0",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.41.0",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.32.8"
  },
  "volta": {
    "node": "14.16.0"
  }
}
